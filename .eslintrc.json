{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/ban-types": ["off"],
        "@typescript-eslint/no-explicit-any": ["off"]
    },
    "parser": "@typescript-eslint/parser"
}
